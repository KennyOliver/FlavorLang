for i in 1..5 {
    serve("Mixing... Step", i);
}

for j in 10..=1 by -3 {
    serve(j);
}


let recipes = ["Cake", "Cookies", "Pasta"];

serve("We have", length(recipes), "recipes today!");

for recipe in recipes {
    serve("Preparing:", recipe);
}

serve("All recipes are ready!");


