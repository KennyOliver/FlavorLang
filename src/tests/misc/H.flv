let x = 5;
let y = 0;

while x > 0 {
    y = y + 1;
    x = x - 1;

    if y > 2 {
        break;
    }
}

serve("X:", x);
serve("Y:", y);


create test() {
    serve("Before");

    for a in 1..=10 {
        if a >= 8 {
            break;
        } else {
            serve(a);
        }
    }

    serve("After");
}

test();
